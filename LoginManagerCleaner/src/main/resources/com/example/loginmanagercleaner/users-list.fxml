         <Font size="18.0" />
      </font>
   </Label>
   <Pane prefHeight="68.0" prefWidth="760.0">
      <children>
         <HBox layoutX="-5.0" layoutY="32.0" prefHeight="36.0" prefWidth="195.0">
            <children>
               <RadioButton contentDisplay="CENTER" mnemonicParsing="false" prefHeight="38.0" prefWidth="190.0" text="Test" textAlignment="CENTER" />
            </children>
         </HBox>
         <HBox layoutX="185.0" layoutY="32.0" prefHeight="36.0" prefWidth="195.0">
            <children>
               <RadioButton mnemonicParsing="false" prefHeight="40.0" prefWidth="190.0" text="Stage" />
            </children>
         </HBox>
         <HBox layoutX="361.0" layoutY="32.0" prefHeight="36.0" prefWidth="204.0">
            <children>
               <RadioButton mnemonicParsing="false" prefHeight="39.0" prefWidth="190.0" text="Production" />
